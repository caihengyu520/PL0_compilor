const a=10;
var i,j,z;
^x:=i;
const b=10;
var m;
begin
    i:=2;
    i++;
    i:=i<<2;
    if a>0 & b>0  then 
	i:=b%5;
    else
          i:=b%3;
    i:=2;
    i:=i+2;
    i+=3;
    write();
    write(j,a);
    j:=x;
    j:=i;
    x:=10;
    j:=x+10;
    j:=j+3;
    read(j);
    read(i);
    z:=x;
    if j>5 then
        j:=5;
    else exit;
    while j>0 do
        j:=j-1;
end.
