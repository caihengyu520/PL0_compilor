const a=10;
var i,j,z;
const b=10;
var m;
begin
    i:=2;
    i++;
    i:=i<<2;
    if a>0 & b>0  then 
	i:=b%5;
    else
          i:=b%3;
    i:=2;
    i:=i+2;
    i+=3;
    write();
    write(j,a);
    j:=3;
    j-=1;
    j+=3;
    j:=j+3;
    read(j);
    if j>5 then
        j:=5;
    else exit;
    while j>0 do
        j:=j-1;
end.
